{
	"name": "nutab",
	"version": "0.0.1",
	"description": "A customizable new tab page for search, bookmark management, and browsing hacker news.",
	"keywords": [
		"svelte",
		"sveltekit",
		"newtab",
		"extension",
		"chrome",
		"browser"
	],
	"type": "module",
	"scripts": {
		"dev": "run-p dev:*",
		"dev:www": "vite dev",
		"dev:proxy": "deno serve --allow-net ./cors-anywhere.ts",
		"dev:sync": "cd sync-worker && pnpm dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"status": "vercel logs https://nutab.fractal-hq.com",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings 'element_invalid_self_closing_tag:ignore,a11y_aria_attributes:ignore,a11y_incorrect_aria_attribute_type:ignore,a11y_unknown_aria_attribute:ignore,a11y_hidden:ignore,a11y_misplaced_role:ignore,a11y_unknown_role:ignore,a11y_no_abstract_role:ignore,a11y_no_redundant_roles:ignore,a11y_role_has_required_aria_props:ignore,a11y_accesskey:ignore,a11y_autofocus:ignore,a11y_misplaced_scope:ignore,a11y_positive_tabindex:ignore,a11y_invalid_attribute:ignore,a11y_missing_attribute:ignore,a11y_img_redundant_alt:ignore,a11y_label_has_associated_control:ignore,a11y_media_has_caption:ignore,a11y_distracting_elements:ignore,a11y_structure:ignore,a11y_mouse_events_have_key_events:ignore,a11y_missing_content:ignore,a11y_click_events_have_key_events:ignore,a11y_no_static_element_interactions:ignore,a11y_no_noninteractive_element_interactions:ignore'",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings 'element_invalid_self_closing_tag:ignore,a11y_aria_attributes:ignore,a11y_incorrect_aria_attribute_type:ignore,a11y_unknown_aria_attribute:ignore,a11y_hidden:ignore,a11y_misplaced_role:ignore,a11y_unknown_role:ignore,a11y_no_abstract_role:ignore,a11y_no_redundant_roles:ignore,a11y_role_has_required_aria_props:ignore,a11y_accesskey:ignore,a11y_autofocus:ignore,a11y_misplaced_scope:ignore,a11y_positive_tabindex:ignore,a11y_invalid_attribute:ignore,a11y_missing_attribute:ignore,a11y_img_redundant_alt:ignore,a11y_label_has_associated_control:ignore,a11y_media_has_caption:ignore,a11y_distracting_elements:ignore,a11y_structure:ignore,a11y_mouse_events_have_key_events:ignore,a11y_missing_content:ignore,a11y_click_events_have_key_events:ignore,a11y_no_static_element_interactions:ignore,a11y_no_noninteractive_element_interactions:ignore' --watch",
		"test:e2e": "playwright test",
		"test:unit": "vitest -- --run",
		"test:unit:watch": "vitest -- --watch",
		"test": "run-p test:e2e test:unit",
		"test:watch": "run-p test:unit:watch"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^5.1.0"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@hapi/hoek": "^11.0.6",
		"@playwright/test": "^1.48.1",
		"@sveltejs/adapter-auto": "^3.3.0",
		"@sveltejs/adapter-static": "3.0.5",
		"@sveltejs/adapter-vercel": "5.4.5",
		"@sveltejs/kit": "^2.7.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/typography": "^0.5.15",
		"@types/bun": "^1.1.12",
		"@types/eslint": "^9.6.1",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/node": "^22.7.9",
		"@types/sjcl": "^1.0.34",
		"autoprefixer": "^10.4.20",
		"cheerio": "1.0.0",
		"cookie": "^1.0.1",
		"cors-anywhere": "^0.4.4",
		"cssnano": "^7.0.6",
		"date-fns": "^4.1.0",
		"dexie": "^4.0.9",
		"dexie-export-import": "^4.1.2",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"esm-env": "^1.0.0",
		"fractils": "^3.1.3",
		"froebel": "^0.23.2",
		"globals": "^15.11.0",
		"greset": "^2.0.0",
		"highlight.js": "^11.10.0",
		"js": "link:@types/@eslint/js",
		"node-html-parser": "6.1.13",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.47",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prismjs": "^1.29.0",
		"pug": "^3.0.3",
		"rimraf": "^6.0.1",
		"sass": "^1.80.4",
		"sjcl": "^1.0.8",
		"svelte": "^5.1.2",
		"svelte-check": "^4.0.5",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.14",
		"tiny-glob": "^0.2.9",
		"tippy.js": "^6.3.7",
		"tslib": "^2.8.0",
		"tsm": "^2.3.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.11.0",
		"vite": "^5.4.10",
		"vitest": "^2.1.3"
	}
}
